    "redis",
    "cache",
    "kachy",
    "multi-tenant",
    "authentication"
  ],
  "author": "Kachy Team <team@kachy.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Klug-Labs/kachy-valkey-node.git"
  },
  "bugs": {
